<!DOCTYPE html>
<html>

<head>

  <title>SmartFan</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

</head>
<body>
    <header>




    </header>

    <div id="videobox" >

      <video autoplay loop poster="http://marketyourbookblog.com/wp-content/uploads/2011/08/target-market2.jpg" id="bgvid">
        <source src="https://s3-sa-east-1.amazonaws.com/cityfolkwtm/Dise%C3%B1os+welcome/Primer+plano/video+fondo.mpeg4">
        <source src="https://s3-sa-east-1.amazonaws.com/cityfolkwtm/Dise%C3%B1os+welcome/Primer+plano/video+fondo.mpeg4">
      </video>
    </div>

</body>


<nav>
  <div class="nav-wrapper">
    <a href="#" class="brand-logo">SmartFan</a>
    <ul id="nav-mobile" class="right hide-on-med-and-down">
      <% if fan_signed_in? %>
          <li>
            <%= link_to current_fan.email , edit_fan_registration_path %>
          </li>
          <li>
            <%= link_to 'Log Out', destroy_fan_session_path, method: :delete %>
          </li>
      <% else %>
          <li class="active">
            <%= link_to 'Sing Up', new_fan_registration_path %>
          </li>
          <li>
            <%= link_to 'Sing In', new_fan_session_path %>
          </li>
      <% end %>
    </ul>
  </div>
</nav>

<p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <%= yield %>

